{
  "name": "turnex-server",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node src/index.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "postinstall": "node -e \"if(process.env.NODE_ENV!=='test'){require('child_process').execSync('prisma generate',{stdio:'inherit'})}\"",
    "migrate:deploy": "prisma migrate deploy",
    "start:render": "npx prisma migrate deploy && node src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "studio": "prisma studio",
    "seed": "node scripts/createInitialUsers.js",
    "open:health": "start \"\" \"http://localhost:3000/api/health\"",
    "dev:open": "concurrently -n API,BROWSER -c blue,green \"npm run dev\" \"wait-on http://localhost:3000/api/health && powershell -NoProfile -Command Start-Process 'http://localhost:3000/api/health'\""
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.6.1",
    "prisma": "^5.17.0",
    "supertest": "^6.3.3",
    "wait-on": "^7.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 20000,
    "transform": {}
  }
}
